[user] 
	email = 
	name = 
[http] 
	sslVerify = false 
[color]  # Ensure we get color
  diff   = auto
  branch = auto
  status = auto
[alias]  # shortcuts
  b      = branch
  c      = checkout
  co     = commit
  d      = diff -b
  dc     = diff -b --cached
  ds     = diff -b --stat
  s      = status
# summary of what you're going to push
  ps     = log --pretty=oneline origin/master..
# like "git log", but include the diffs
  w      = whatchanged -p
# changes since we last did a push
  wo     = whatchanged -p origin..
